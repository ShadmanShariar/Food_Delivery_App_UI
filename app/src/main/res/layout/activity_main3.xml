        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/o4" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="154dp"
        android:layout_height="179dp"
        android:layout_marginTop="16dp"
